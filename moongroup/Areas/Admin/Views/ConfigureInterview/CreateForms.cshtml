    <div class="modal-dialog " role="document">
        <div class="modal-content">
            <div class="modal-header">
                <h5 class="modal-title" id="addSectionTitle"> Section </h5>
                <button type="button" class="close" data-dismiss="modal" aria-label="Close">
                    <span aria-hidden="true">&times;</span>
                </button>
            </div>
            <div class="modal-body">
                <input type="hidden" id="updateSectionId" />
                <div class="row">
                    <div class="col-md-12">
                        <div class="form-group">
                            <label>Title</label>
                            <input type="text" id="Name" class="form-control" required="" placeholder="Enter Section Title... ">
                        </div>
                    </div>
                    <div class="col-md-12">
                        <div class="form-group">
                            <div class="custom-control custom-checkbox">
                                <input type="checkbox" class="custom-control-input" id="section_is_required" required="">
                                <label class="custom-control-label" for="section_is_required">Is Required</label>
                            </div>
                        </div>
                    </div>
                </div>
                <div style="float:right">
                    <button class="btn btn-danger m-t-15 waves-effect" data-dismiss="modal">Close</button>
                    <button type="submit" onclick="AddSection()" id="add_section" class="btn btn-primary m-t-15 waves-effect">Save</button>
                    <button type="submit" onclick="UpdateSection()" style="display:none;" id="edit_section" class="btn btn-primary m-t-15 waves-effect">Save</button>
                </div>
            </div>
        </div>
    </div>
</div>
<div class="modal fade" id="addNewFieldModal" tabindex="-1" role="dialog" aria-labelledby="formModal"
     aria-hidden="true">
    <div class="modal-dialog modal-lg" role="document">
        <div class="modal-content">
            <div class="modal-header">
                <h5 class="modal-title" id="fieldSetting">Field Setting</h5>
                <button type="button" class="close" data-dismiss="modal" aria-label="Close">
                    <span aria-hidden="true">&times;</span>
                </button>
            </div>
            <div class="modal-body">
                <input type="hidden" id="sectionId" />
                <input type="hidden" id="FieldId" />
                <input type="hidden" id="FieldType" />
                <div class="row">
                    <div class="col-md-6">
                        <div class="form-group">
                            <label>Label</label>
                            <input type="text" id="label" class="form-control" placeholder="Enter Field Label... ">
                        </div>
                    </div>
                    <div class="col-md-6">
                        <div class="form-group">
                            <label>Helper Text</label>
                            <input type="text" id="helperText" class="form-control" placeholder="Enter Field Label... ">
                        </div>
                    </div>
                    <div class="col-md-6">
                        <div class="form-group">
                            <label>Placeholder</label>
                            <input type="text" id="placeholder" class="form-control" placeholder="Enter Field Placeholder... ">
                        </div>
                    </div>
                    <div class="col-md-6 updateFieldSection">
                        <div class="form-group">
                            <label>Select Type</label>
                            <select onchange="ChangeSelectType()" id="select_field_type" class="form-control">
                                <option selected disabled value="">Select Field Type</option>
                                <option value="1">Text</option>
                                <option value="2">Number</option>
                                <option value="3">Textarea</option>
                                <option value="4">Select</option>
                                <option value="5">Checkbox</option>
                                <option value="6">Radio</option>
                                <option value="7">Password</option>
                                <option value="8">GroupField</option>
                                <option value="9">Is multiple section</option>
                                <option value="10">Date Field</option>
                                <option value="11">SSN Mask Field</option>
                                <option value="12">Dollar Field</option>
                            </select>
                        </div>
                    </div>
                    <div style="display:none;" class="col-md-6 number_field">
                        <div class="form-group">
                            <label>Maximum</label>
                            <input type="number" id="maximum" class="form-control" placeholder="Enter Maximum Value... ">
                        </div>
                    </div>
                    <div style="display:none;" class="col-md-6 number_field">
                        <div class="form-group">
                            <label>Minimum</label>
                            <input type="number" id="minimum" class="form-control" placeholder="Enter Minimum Value... ">
                        </div>
                    </div>
                    <div style="display:none;" class="col-md-12 select_field">
                        <div class="form-group">
                            <div class="custom-control custom-checkbox">
                                <input type="checkbox" class="custom-control-input" id="is_multiple" required="">
                                <label class="custom-control-label" for="is_multiple">Is select multiple</label>
                            </div>
                        </div>
                    </div>

                    <div class="col-md-12">
                        <div class="form-group">
                            <div class="custom-control custom-checkbox">
                                <input type="checkbox" class="custom-control-input" id="is_required" required="">
                                <label class="custom-control-label" for="is_required">Is Required </label>
                            </div>
                        </div>
                    </div>
