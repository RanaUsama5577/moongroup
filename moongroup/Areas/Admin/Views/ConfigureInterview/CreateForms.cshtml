<script>
    var getResponseFromUrl = function (requestType, requestUrl, requestData) {
        var response;
        return $.ajax({
            type: requestType,
            url: requestUrl,
            data: requestData,
            async: true,
            processData: false,
            contentType: false,
            success: function (data) {
                response = data;
                console.log(data);
                return response;
            },
            error: function (error) {
                return error;
            },
        });
    }
</script>