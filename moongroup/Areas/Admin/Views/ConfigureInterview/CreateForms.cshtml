<script>
    $(function () {
        $('#interview').addClass('active');
        AppendForms();
        setTimeout(function () {
            new Cleave($('.ssn'), { blocks: [3, 3, 3], uppercase: !0 });
        }, 2000);
        $('.form-password-toggle .input-group-text').on('click', function (e) {
            e.preventDefault();
            var a = $(this);
            var n = $(a).closest('.form-password-toggle');
            var o = $(a).closest('.form-password-toggle').find('input');
            "password" === o.attr("type") ? (o.attr("type", "text"), n.find("i").addClass('fa-eye-slash').removeClass('fa-eye')) : "text" === o.attr("type") && (o.attr("type", "password"), n.find("i").addClass('fa-eye').removeClass('fa-eye-slash'));
        })
    })
    function AppendForms() {
        $('#sectionAppend').html("");
        $.ajax({
            type: 'Get',
            url: '@Url.Action("AppendForms", "ConfigureInterview",new {Id = ViewBag.Id})',
            data: null,
            async: true,
            processData: false,
            contentType: false,
            success: function (data) {
                console.log(data);
                if (data.length > 0) {
                    data.forEach(function (item) {
                        var clone = $('#SectionClone').clone();
                        clone.find('#editSection').attr('data-edit', item.id);

                        var values = '<input type="hidden" id="n-' + item.id + '" value="' + item.name + '"><input type="hidden" id="r-' + item.id + '" value="' + item.isRequired +'">';
                        clone.find('#editSection').attr('data-edit', item.id);
                        clone.find('#editSection').append(values);
                        clone.find('#deleteSection').attr('data-delete', item.id);
                        clone.find('#addNewField').attr('data-addField', item.id);
                        clone.find('#sectionName').html(item.name);
                        clone.find('#sectionRequirement').html(item.isRequired);
                        clone.find('#sectionAcc').attr('data-target', '#accordian-' + item.id);
                        clone.find('#sectionAcc').attr('id', 'accordianWrap-' + item.id);
                        clone.find('#sectionAccChild').attr('id', 'accordian-' + item.id);
                        clone.find('#sectionAccChild').attr('data-parent', '#accordianWrap-' + item.id);

                        if (item.sectionFields.length > 0) {
                            var fields = GetFields(item.sectionFields);
                            clone.find('#fieldsOfSection').html(fields);
                        }
                        clone.show();
                        clone.attr('id', "");
                        $('#sectionAppend').append(clone);
                    })
                }
            },
            error: function (error) {
                console.log(error);
            },
        });
    }
