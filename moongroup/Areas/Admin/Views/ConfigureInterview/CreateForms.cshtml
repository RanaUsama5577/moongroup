    function GetFormsFieldOptions(options,num,id) {
        var result = "";
        if (num == 3) {
            options.forEach(function (item) {
                var option = "<option value='" + item.value + "'>" + item.text + "</option>";
                result += option;
            })
        }
        else if (num == 4) {
            options.forEach(function (item) {
                var option = `<div class="custom-control custom-checkbox">
                                <input type="checkbox" name="i-${id}" class="custom-control-input" id="${item.id}" required="">
                                <label class="custom-control-label" for="${item.id}"> ${item.text} </label>
                            </div>`;
                result += option;
            })
        }
        else if (num == 5) {
            var counaa = 0;
            options.forEach(function (item) {
                counaa++;
                if (counaa == 1) {
                    var option = `<div class="custom-control custom-checkbox">
                                <input type="radio" checked name="i-${id}" class="custom-control-input" id="${item.id}" required="">
                                <label class="custom-control-label" for="${item.id}"> ${item.text} </label>
                            </div>`;
                }
                else {
                    var option = `<div class="custom-control custom-checkbox">
                                <input type="radio" name="i-${id}" class="custom-control-input" id="${item.id}" required="">
                                <label class="custom-control-label" for="${item.id}"> ${item.text} </label>
                            </div>`;
                }
                result += option;
            })
        }
        return result;
    }
